<p id="notice"><%= notice %></p>

<h1>Ufolders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Parent</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ufolders.each do |ufolder| %>
      <tr>
        <td><%= ufolder.name %></td>
        <td><%= ufolder.parent_id %></td>
        <td><%= ufolder.user_id %></td>
        <td><%= link_to 'Show', ufolder %></td>
        <td><%= link_to 'Edit', edit_ufolder_path(ufolder) %></td>
        <td><%= link_to 'Destroy', ufolder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ufolder', new_ufolder_path %>
